; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Dart Board Games"
run/main_scene="res://MenuScreen.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

UiSoundManager="*res://UISounds.tscn"
SceneManager="*res://SceneManager.gd"
ProfileSystem="*res://ProfileSystem.gd"
GameManager="*res://GameManager.tscn"

[display]

window/size/viewport_width=1024
window/size/viewport_height=600
window/size/mode=3
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Dart Board Games"

[editor_plugins]

enabled=PackedStringArray("res://addons/onscreenkeyboard/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
